{
    "name": "padosoft/laravel-settings",
    "description": "Persistent settings with caching in Laravel.",
    "license": "MIT",
    "keywords": [
        "padosoft",
        "laravel-settings",
        "laravel",
        "settings",
        "cache"
    ],
    "authors": [
        {
            "name": "Lorenzo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        },
        {
            "name": "Leonardo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/contracts": ">=5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/encryption": ">=5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/validation": ">=5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "doctrine/dbal": "^3.3",
        "illuminate/database": ">=5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "genealabs/laravel-model-caching": "0.11|>=0.12 <0.14"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^3.8.2|^4.0|^5.0|^6.0|^7.0|^8.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Padosoft\\Laravel\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\Laravel\\Settings\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "aliases": {
                "SettingsManager": "Padosoft\\Laravel\\Settings\\Facade"
            },
            "providers": [
                "Padosoft\\Laravel\\Settings\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
